<h1>Editing version <%=h @version.id %></h1>

<%= form_for(@version) do |f| %>
<% @version.errors.full_messages.each do |msg| %>
    <p>Error: <%= msg %></p>
<% end %>

  <%= render :partial => 'version', :locals => {:f => f} %>
  
  <%= f.submit "Update" %>
<% end %>

<%= link_to 'Back', versions_path %>
